@using Microsoft.AspNetCore.Identity
@model List<IdentityUser>

@{
    ViewBag.Title = "List";

}
<div class="row clearfix">
    <div class="col-lg-3">
        <div class="widget style2 bg-green hover-zoom-effect">
            <div class="col-xs-4 widget-icon">
                <i class="material-icons">format_list_bulleted</i>
            </div>
            <div class="col-xs-8 widget-body text-right">
                <span>Üye Sayısı </span>
                <h2 class="num count-to" data-from="0" data-to="@Model.Count" data-speed="1000" data-fresh-interval="20">@Model.Count</h2>
            </div>
        </div>
    </div>
</div>

<!-- Basic Examples -->
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="card-header">
                <h4>
                    Üye Listesi
                </h4>
                <small>Üyeler listelenir.</small>
            </div>
            <div class="body">
                <table class="table table-bordered table-striped table-hover basic-example dataTable">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Üye</th>
                            <th>Eposta</th>
                            <th>Telefon</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Id</th>
                            <th>Üye</th>
                            <th>Eposta</th>
                            <th>Telefon</th>
                        </tr>
                    </tfoot>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Id</td>
                                <td>@item.UserName</td>
                                <td>@item.Email</td>
                                <td>@item.PhoneNumber</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- #END# Basic Examples -->
@section css {
    <!--THIS PAGE LEVEL CSS-->
